<% @notifications.each do |notification| %>
  <% if notification.message == 'risk' %>
    <div class="d-flex" style="background-color: rgba(255, 234, 112, 0.3)">
      <p class="mr-3">⚠️</p>
      <p class="p-3 m-2">Your favorite parking <%= notification.parking.name %>'s safety level dropped below 40%</p>
    </div>
  <% end %>
<% end %>